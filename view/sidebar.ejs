<style>
    .nk-sidebar .metismenu>li.active>a {
      background: #4AC1EE;
      color: #464a53;
    }
  
    .btn-outline-primary {
      color: #4AC1EE;
      background-color: transparent;
      background-image: none;
      border-color: #4AC1EE;
    }
  
    .btn-outline-primary:hover {
      background: #4AC1EE;
      color: #fff;
      border-color: #4AC1EE;
    }
  
    .btn-primary {
      color: #fff;
      background-color: #4AC1EE;
      border-color: #4AC1EE;
    }
  
    .page-item.active .page-link {
