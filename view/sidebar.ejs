      border-color: #4AC1EE;
    }
  
    .page-link {
  
      color: #4AC1EE;
      background-color: #fff;
      border: 1px solid #dee2e6;
    }
  
    .page-link:hover {
      z-index: 2;
      color: #4AC1EE;
      text-decoration: none;
      background-color: #e9ecef;
      border-color: #dee2e6;
    }
  
    .btn-primary:active,
    .btn-primary:focus,
    .btn-primary:hover {
      background: #4AC1EE;
      color: #fff;
      border-color: #4AC1EE;
    }
    .btn-outline-primary {
    
      border-color: #4AC1EE;
  }
  .nk-sidebar .metismenu > li:hover i, .nk-sidebar .metismenu > li:focus i, .nk-sidebar .metismenu > li.active i {
      color: #fff !important;
  }
  .nk-sidebar .metismenu > li:hover span, .nk-sidebar .metismenu > li:focus span, .nk-sidebar .metismenu > li.active span {
  
      color: #fff;
  }
  .nk-sidebar .metismenu > li:hover > a {
      background: #4AC1EE !important;
      color: #4AC1EE;
  }
