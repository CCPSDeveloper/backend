  <% include ../header.ejs %>
  <% include ../navbar.ejs %>
  <% include ../sidebar.ejs %>

  <div class="content-body">

            <div class="row page-titles mx-0">
                <div class="col p-md-0">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="javascript:void(0)">Dashboard</a></li>
                        <li class="breadcrumb-item active"><a href="/admin/provider">Back</a></li>
                    </ol>
                </div>
            </div>
            <!-- row -->

            <div class="container-fluid">
                <div class="row">

 <div class="col-lg-12">
                        <div class="card">
                            <div class="card-body">
                                <h4 class="card-title">Provider View</h4>
                                <div class="table-responsive">
                                    <table class="table header-border">
                                       
                                        <tbody>
                                            <tr>
                                                <th>Name</th>
                                                <td><%=response.dataValues.firstName%></td>
                                               
                                            </tr>
                                            <tr>
                                                <th>Email</th>
                                                <td><%=response.dataValues.email%></td>
                                                
                                            </tr>

                                            <tr>
                                                <th>Category</th>
                                                <td><%=response.dataValues.category.name%></td>
                                              
                                            </tr>
                                            <tr>
                                                <th>Image</th>
                                               
                                                <td><img src="<%= '/upload/'+response.dataValues.image%>" class="img-rounded" alt="image" value="" width="80" height="80"></td>
                                                
                                            </tr>

                                            <tr>
                                                <th>Business Licence</th>
                                               
                                                <td><img src="<%= '/upload/'+response.dataValues.businessLicence %>" class="img-rounded" alt="image" value="" width="80" height="80"></td>
                                                
                                            </tr>

                                            <tr>
                                                <th>Insurance</th>
                                               
                                                <td><img src="<%= '/upload/'+response.dataValues.insurance%>" class="img-rounded" alt="image" value="" width="80" height="80"></td>
                                                
                                            </tr>


                                            <tr>
                                                <th>Resume</th>
                                               
                                                <td><img src="<%= '/upload/'+response.dataValues.resume%>" class="img-rounded" alt="image" value="" width="80" height="80"></td>
                                                
                                            </tr>
                                            <tr>
                                                <th>Status</th>
                                                <td><%=(response.dataValues.status == 1) ? 'Active' : 'Inactive' %></td>
                                               
                                            </tr>
                                            
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <% include ../footer.ejs %>